<template>
    <a-card :bordered="false">
#set($has_query = false)
#foreach ($component in $components)
    #if(${component.type} == "QUERY")
        #set($has_query = true)
        #set($query_component = $component)
        #break
    #end
#end
#foreach ($component in $components)
  #if(${component.type} == "LIST")
    #set($list_component = $component)
    #break
  #end
#end
#if($has_query)

        <!-- 查询区域 -->
        <div class="table-page-search-wrapper">
            <a-form layout="inline">
                <a-row :gutter="24">
#foreach($field in $query_component.fields)

                    <a-col :md="${field.view.span}">
                        <a-form-item label="${field.label}">
                            <a-input #if(${field.view.placeholder})placeholder="${field.view.placeholder}"#end v-model="queryParam.${field.classname}"></a-input>
                        </a-form-item>
                    </a-col>
#end

                    <span style="float: left;overflow: hidden;" class="table-page-search-submitButtons">
                        <a-col :md="6" :sm="24">
                            <a-button type="primary" @click="searchQuery" icon="search">查询</a-button>
                             <a-button type="primary" @click="searchReset" icon="reload" style="margin-left: 8px">重置</a-button>
                        </a-col>
                    </span>
                </a-row>
            </a-form>
        </div>
#end

        <!-- 操作按钮区域 -->
        <div class="table-operator" style="margin-top: 5px">
#set($_count=0)
#foreach($action in $list_component.actions)
  #set($_count=$_count+1)
  #if(${action.type} == "OPEN-VIEW")

            <a-button @click="clickButton${_count}" type="primary" #if(${action.icon})icon="${action.icon}"#end>${action.label}</a-button>
  #elseif(${action.type} == "DO-SERVICE")

            <a-button @click="clickButton${_count}" type="primary" #if(${action.icon})icon="${action.icon}"#end>${action.label}</a-button>
  #elseif(${action.type} == "EXPORT-EXCEL")

            <a-button @click="clickButton${_count}" type="primary" #if(${action.icon})icon="${action.icon}"#end>${action.label}</a-button>
  #elseif(${action.type} == "IMPORT-EXCEL")

            <a-upload name="file" :showUploadList="false" :multiple="false" :action="importExcelUrl" @change="clickButton${_count}">
                <a-button type="primary" #if(${action.icon})icon="${action.icon}"#end>${action.label}</a-button>
            </a-upload>
  #end
#end

        </div>

        <!-- table区域-begin -->
        <div>
            <div class="ant-alert ant-alert-info" style="margin-bottom: 16px;">
                <i class="anticon anticon-info-circle ant-alert-icon"></i> 已选择 <a style="font-weight: 600">{{
                selectedRowKeys.length }}</a>项
                <a style="margin-left: 24px" @click="onClearSelected">清空</a>
            </div>

            <a-table
                    ref="table"
                    size="middle"
                    bordered
                    rowKey="${list_component.primaryKeyField.classname}"
                    :columns="columns"
                    :dataSource="dataSource"
                    :pagination="ipagination"
                    :loading="loading"
                    :rowSelection="{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}"
                    @change="handleTableChange">

              <span slot="action" slot-scope="text, record">
#set($_count=0)
#set($_flag=false)
          #foreach($action in $list_component.actions)
            #set($_count=$_count+1)
            #if(${action.type} == "LIST-OPEN-VIEW")
                #if($_flag)<a-divider type="vertical"/>#else #set($_flag=true) #end

                <a @click="clickButton${_count}(record)">${action.label}</a>
            #elseif(${action.type} == "LIST-DO-SERVICE")

                #if($_flag)<a-divider type="vertical"/>#else #set($_flag=true) #end

                <a @click="clickButton${_count}(record)">${action.label}</a>
            #elseif(${action.type} == "LIST-DELETE")

                #if($_flag)<a-divider type="vertical"/>#else #set($_flag=true) #end

                <a-popconfirm title="确定删除吗?" @confirm="() => clickButton${_count}(record)">
                  <a>${action.label}</a>
                </a-popconfirm>
            #end
          #end

              </span>
            </a-table>
        </div>
        <!-- table区域-end -->
    </a-card>
</template>
<script>
    import { MdaListMixin } from '@/utils/MdaListMixin.js'
    import {filterDictText} from '@/components/dict/JDictSelectUtil'
    import {dicts} from '@/config/MdaDictionary.js'
    import eventBus from '@/utils/eventBus.js'
    import { putAction, postAction, deleteAction, getAction } from '@/api/manage'

    export default {
        name: "${className}",
        mixins:[MdaListMixin],
        components: {
        },
        data() {
            return {
                description: '${label}',
                //字典数组缓存
                selectOptions: {},
                importExcelUrl:`${usd}{window._CONFIG['domianURL']}/${module}/${list_component.classname}/importExcel`,
                // 表头
                columns: [
                    {
                        title: '#',
                        dataIndex: '',
                        key: 'rowIndex',
                        width: 60,
                        align: "center",
                        customRender: function (t, r, index) {
                            return parseInt(index) + 1;
                        }
                    },
                    #foreach($field in ${list_component.fields})

                    {
                        title: '${field.label}',
                        align: "center",
                        dataIndex: '${field.classname}'#if(${field.inputType} == "select"),
                        customRender: (text) => {
                            //字典值替换通用方法
                            return filterDictText(this.selectOptions['${field.dictName}'], text);
                        }
#end

                    },
                    #end
                    {
                        title: '操作',
                        dataIndex: 'action',
                        align: "center",
                        scopedSlots: {customRender: 'action'},
                    }
                ],
                url: {
                    list: "/${module}/${list_component.classname}/list",
                    delete: "/${module}/${list_component.classname}/deleteMain",
                    deleteBatch: "/${module}/${list_component.classname}/deleteBatchMain",
                },
            }
        },
        created () {
            this.selectOptions = dicts;
            //eventBus.$on('${module}-${name}', target => {
            //    console.log("${list_component.view.editFormName}:"+target);
            // });
        },
        methods: {
      #set($_count=0)
      #foreach($action in $list_component.actions)
        #set($_count=$_count+1)
        #if(${action.type} == "OPEN-VIEW")

        clickButton${_count}() {
          #if(${action.params.query})

              let query = ${action.params.query};
              this.${usd}router.push({path:'${action.params.view}',query:query});
          #else

              this.${usd}router.push({path:'${action.params.view}'});
          #end

        },
        #elseif(${action.type} == "DO-SERVICE")

        clickButton${_count}() {
            let data = #if(${action.params.data})${action.params.data}#else {}#end;
            ${action.params.method}Action("${action.params.url}", data).then((res) => {
                if (res.success) {
                    this.${usd}message.success(res.message);
                    this.loadData();
                } else {
                    this.${usd}message.warning(res.message);
                }
            });
        },
        #elseif(${action.type} == "EXPORT-EXCEL")

        clickButton${_count}() {
            let paramsStr = encodeURI(JSON.stringify(this.getQueryParams()));
            let url = `${usd}{window._CONFIG['domianURL']}/${list_component.table.module}/${list_component.table.classname}/exportXls?paramsStr=${paramsStr}`;
            window.location.href = url;
        },
        #elseif(${action.type} == "IMPORT-EXCEL")

        clickButton${_count}(info){
            if (info.file.status !== 'uploading') {
                console.log(info.file, info.fileList);
            }
            if (info.file.status === 'done') {
                this.$message.success("${info.file.name} 文件上传成功");
                this.loadData();
            } else if (info.file.status === 'error') {
                this.$message.error("${info.file.name} 文件上传失败.");
            }
        },
        #elseif(${action.type} == "LIST-OPEN-VIEW")

        clickButton${_count}(record) {
          #if(${action.params.query})

              let query = ${action.params.query};
              this.${usd}router.push({path:'${action.params.view}',query:query});
          #else

              this.${usd}router.push({path:'${action.params.view}'});
          #end

        },
        #elseif(${action.type} == "LIST-DO-SERVICE")

        clickButton${_count}(record) {
            let data = #if(${action.params.data})${action.params.data}#else {}#end;
            ${action.params.method}Action("${action.params.url}", data).then((res) => {
                if (res.success) {
                    this.${usd}message.success(res.message);
                    this.loadData();
                } else {
                    this.${usd}message.warning(res.message);
                }
            });
        },
        #elseif(${action.type} == "LIST-DELETE")

        clickButton${_count}(record) {
            handleDelete(record.${list_component.table.primaryKeyField.classname});
        },
        #end
      #end


        }
    }
</script>
<style scoped>
    .ant-card-body .table-operator {
        margin-bottom: 18px;
    }

    .ant-table-tbody .ant-table-row td {
        padding-top: 15px;
        padding-bottom: 15px;
    }

    .anty-row-operator button {
        margin: 0 5px
    }

    .ant-btn-danger {
        background-color: #ffffff
    }

    .ant-modal-cust-warp {
        height: 100%
    }

    .ant-modal-cust-warp .ant-modal-body {
        height: calc(100% - 110px) !important;
        overflow-y: auto
    }

    .ant-modal-cust-warp .ant-modal-content {
        height: 90% !important;
        overflow-y: hidden
    }
    /** Button按钮间距 */
    .ant-btn {
        margin-left: 3px
    }
</style>